@model IEnumerable<WebApplication1.Models.EmployeeSalaryDto>

    <h2>Employee Salary Details</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Full Name</th>
                <th>Company Name</th>
                <th>Net Salary</th>
                <th>Gross Salary</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FullName</td>
                <td>@item.CompanyName</td>
                <td>@item.NetSalary</td>
                <td>@item.GrossSalary</td>
            </tr>
        }
        </tbody>
    </table>
